# config.toml

[server]
headless = true
port = 8501
enableCORS = false
enableWebsocketCompression = false

[theme]
# Theme settings for your app (you can customize colors here)
primaryColor = "#1f77b4"
backgroundColor = "#ffffff"
secondaryBackgroundColor = "#f0f2f6"
textColor = "#000000"
font = "sans serif"

[logger]
# Configure logger settings
level = "error"
